                textLookingAt.setText("");
                textLookingAt.setTextSize(textSize);
                if(android.os.Build.VERSION.SDK_INT > 19){ // KITKAT
					textLookingAt.setShadowLayer(1, Math.round(textLookingAt.getLineHeight() / 8), Math.round(textLookingAt.getLineHeight() / 8), android.graphics.Color.parseColor("#FF333333"));
				}else{
					textLookingAt.setShadowLayer(0.0001, Math.round(textLookingAt.getLineHeight() / 8), Math.round(textLookingAt.getLineHeight() / 8), android.graphics.Color.parseColor("#FF333333"));
				}
                textLookingAt.setTextColor(android.graphics.Color.WHITE);
                textLookingAt.setTypeface(mcfont);

textBlockInfo = new android.widget.TextView(ctx);
                textBlockInfo.setText("");
                textBlockInfo.setTextSize(textSize);
                if(android.os.Build.VERSION.SDK_INT > 19){ // KITKAT
					textBlockInfo.setShadowLayer(1, Math.round(textBlockInfo.getLineHeight() / 8), Math.round(textBlockInfo.getLineHeight() / 8), android.graphics.Color.parseColor("#FF333333"));
				}else{
					textBlockInfo.setShadowLayer(0.0001, Math.round(textBlockInfo.getLineHeight() / 8), Math.round(textBlockInfo.getLineHeight() / 8), android.graphics.Color.parseColor("#FF333333"));
				}
                textBlockInfo.setTextColor(android.graphics.Color.WHITE);
                textBlockInfo.setTypeface(mcfont);

textDamage = new android.widget.TextView(ctx);
                textDamage.setText("");
                textDamage.setTextSize(textSize);
                if(android.os.Build.VERSION.SDK_INT > 19){ // KITKAT
					textDamage.setShadowLayer(1, Math.round(textDamage.getLineHeight() / 8), Math.round(textDamage.getLineHeight() / 8), android.graphics.Color.parseColor("#FF333333"));
				}else{
					textDamage.setShadowLayer(0.0001, Math.round(textDamage.getLineHeight() / 8), Math.round(textDamage.getLineHeight() / 8), android.graphics.Color.parseColor("#FF333333"));
				}
                textDamage.setTextColor(android.graphics.Color.WHITE);
                textDamage.setTypeface(mcfont);
                layout.setOrientation(1);
                layout.addView(textVersion);
                layout.addView(textFPS);
                layout.addView(textChunk);
                layout.addView(textObjects);
                layout.addView(textUpdates);
                layout.addView(textMultiplayer);
                layout.addView(textN1);
                layout.addView(textPosition);
                layout.addView(textPosition2);
                layout.addView(textChunk2);
                layout.addView(textFacing);
                layout.addView(textBiome);
                layout.addView(textLight);
                layout.addView(textDifficult);
                layout.addView(textLookingAt);
                layout.addView(textBlockInfo);
                layout.addView(textDamage);
                simpleGUI.setContentView(layout);
                simpleGUI.setWidth(displayWidth);
                simpleGUI.setHeight(displayHeight);
                simpleGUI.setBackgroundDrawable(null);
                simpleGUI.setTouchable(false);
                simpleGUI.showAtLocation(ctx.getWindow().getDecorView(), android.view.Gravity.LEFT | android.view.Gravity.TOP, displayWidth / 384, 0);
                show = true;
            } else if (show == true) {
                simpleGUI.dismiss();
                show = false;
            }

        }
    })
}

function leaveGame() {
    ctx.runOnUiThread(new java.lang.Runnable() {
        run: function () {
            if (show == true) {
                simpleGUI.dismiss();
            }
            GUIButton.dismiss();
        }
    })
}
