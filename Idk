var ctx = com.mojang.minecraftpe.MainActivity.currentMainActivity.get();
var textVersion;
var textFPS;
var textSize=7.5;

var fps = 0;
var option;
var difficult;
var distance;
var clockWork=100;
var updateClock=clockWork-1;
var lastLoop = new Date;
var pathFont = android.os.Environment.getExternalStorageDirectory().getAbsolutePath() + "/games/com.mojang";
var fontFont = "  ";
var buttonButton = "  "
var mcfont;
var Compass = "south (Towards positive Z)";
var show = false;

function newLevel() {
    Base64Decode(android.util.Base64.decode(fontFont, 0), pathFont + "/minecraft.ttf");
    Base64Decode(android.util.Base64.decode(buttonButton, 0), pathFont + "/button.png");
    var img1 = new android.graphics.BitmapFactory.decodeFile("mnt/sdcard/games/com.mojang/button.png");
    var buttonGUI = new android.graphics.drawable.BitmapDrawable(img1);

    ctx.runOnUiThread(new java.lang.Runnable() {
        run: function () {
            var metrics = new android.util.DisplayMetrics();
            ctx.getWindowManager().getDefaultDisplay().getMetrics(metrics);
            var displayHeight = metrics.heightPixels;
            var displayWidth = metrics.widthPixels;

            try {
                GUIButton = new android.widget.PopupWindow();
                var layoutButton = new android.widget.LinearLayout(ctx);
                layoutButton.setOrientation(android.widget.LinearLayout.VERTICAL);
                GUIButton.setContentView(layoutButton);
                GUIButton.setWidth(displayWidth / 19.2);
                GUIButton.setHeight(displayHeight / 10.8);
                var btnButton = new android.widget.Button(ctx);
                layoutButton.addView(btnButton);
                btnButton.setAlpha(0.55);
                btnButton.setBackgroundDrawable(buttonGUI);

                btnButton.setOnClickListener(new android.view.View.OnClickListener({
                    onClick: function (viewarg) {
                        showDebug();
                        Level.playSound(Player.getX(), Player.getY(), Player.getZ(), "random.break", 100, 100);
                    }
                }))

                btnButton.setText("\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n");
                GUIButton.showAtLocation(ctx.getWindow().getDecorView(), android.view.Gravity.RIGHT | android.view.Gravity.TOP, displayWidth / 192, displayHeight / 10.8);
            } catch (err) {
                print(err)
            }
        }
    })
}

function Base64Decode(byteArray, Path) {
    try {
        var File = new java.io.File(Path);
        if (!File.exists()) {
            File.createNewFile();
            var Stream = new java.io.FileOutputStream(File);
            Stream.write(byteArray);
            Stream.close();
        }
        mcfont = new android.graphics.Typeface.createFromFile(pathFont + "/minecraft.ttf")
    } catch (err) {
        print(err);
    }
}

function modTick() {
    if (Entity.getYaw(getPlayerEnt()) >= 360) {
        Entity.setRot(getPlayerEnt(), Entity.getYaw(getPlayerEnt()) - 360, Entity.getPitch(getPlayerEnt()))
    } else if (Entity.getYaw(getPlayerEnt()) < 0) {
        Entity.setRot(getPlayerEnt(), Entity.getYaw(getPlayerEnt()) + 360, Entity.getPitch(getPlayerEnt()))
    }
    if ((Entity.getYaw(getPlayerEnt()) >= 315 && Entity.getYaw(getPlayerEnt()) < 360)||(Entity.getYaw(getPlayerEnt()) >= 0 && Entity.getYaw(getPlayerEnt()) < 45)) {
        Compass = "south (Towards positive Z)";
    } else if (Entity.getYaw(getPlayerEnt()) >= 45 && Entity.getYaw(getPlayerEnt()) < 135) {
        Compass = "west (Towards negative X)";
    } else if (Entity.getYaw(getPlayerEnt()) >= 135 && Entity.getYaw(getPlayerEnt()) < 225) {
        Compass = "north (Towards negative Z)";
    } else if (Entity.getYaw(getPlayerEnt()) >= 225 && Entity.getYaw(getPlayerEnt()) < 315) {
        Compass = "east (Towards positive X)";
    }
    var memfree=Math.floor(java.lang.Runtime.getRuntime().freeMemory()/1024);
    var memtotal=Math.floor(java.lang.Runtime.getRuntime().totalMemory()/1024);
    gameLoop();
    getOption();
    if (show == true) {
        ctx.runOnUiThread(new java.lang.Runnable() {
            run: function () {
                textVersion.setText("Minecraft Pocket Edition " + ModPE.getMinecraftVersion() + " alpha    Mem: " + Math.floor((memtotal - memfree) / memtotal*100) + "% " + (memtotal-memfree) + "/" + memtotal + "KB");
                textFPS.setText(Math.round(fps) + " fps (0 chunk updates) T: 20");
                textChunk.setText("C: 0/4000 (s) D: " + distance + ", pC: 000, pU: 0, aB: 1");
                textObjects.setText("E: 0/" + Entity.getAll().length + ", B: 0, I: 0");
                textUpdates.setText("P: 0, T: All: " + Entity.getAll().length);
                textPosition.setText("XYZ: " + Math.round(Player.getX() * 100) / 100 + " / " + Math.round(Player.getY() * 100) / 100 + " / " + Math.round(Player.getZ() * 100) / 100);
                textPosition2.setText("Block: " + Math.round(Player.getX()) + " / " + Math.round(Player.getY()) + " / " + Math.round(Player.getZ()));
                textChunk2.setText("Chunk: " + Math.floor(Math.round(Player.getX()) % 16) + " " + Math.floor(Math.round(Player.getY()) % 16) + " " + Math.floor(Math.round(Player.getZ()) % 16) + " in " + Math.floor(Math.round(Player.getX()) / 16) + " / " + Math.floor(Math.round(Player.getY()) / 16) + " / " + Math.floor(Math.round(Player.getZ()) / 16));
                textFacing.setText("Facing: " + Compass + " (" + Math.round(Entity.getYaw(getPlayerEnt())) + " / " + Math.round(Entity.getPitch(getPlayerEnt())) + ")");
                textBiome.setText("Biome: " + Level.getBiomeName(Player.getX(),Player.getZ()));
                textLight.setText("Light: " + Level.getBrightness(Player.getX(), Player.getY() - 1, Player.getZ()));
                textDifficult.setText("Local Difficulty: " + difficult + " (Day X)");
                textLookingAt.setText("Looking at: X: " + Player.getPointedBlockX() + " Y: " + Player.getPointedBlockY() + " Z: " + Player.getPointedBlockZ());
                textBlockInfo.setText(getId());
                if(Player.getPointedBlockData()==-1){
                  textDamage.setText(" ");
                }else{
                textDamage.setText("Damage: " + Player.getPointedBlockData());
                }
            }
        })
    }
}

function gameLoop() {
    var thisLoop = new Date;
    fps = 1000 / (thisLoop - lastLoop);
    lastLoop = thisLoop;
}

function getOption() {
updateClock++;
if(updateClock == clockWork){
updateClock = 0;
option = (function () {
var path = pathFont + "/minecraftpe/options.txt",
cont = "",
file = new java.io.File(path),
fis = new java.io.FileInputStream(file),
str = new java.lang.StringBuilder(),
char;
while((char = fis.read()) != -1){
str.append(java.lang.Character(char));
}
cont = String(str.toString());
fis.close();
return cont.split("\n");
}());
for (i = 0;i < option.length;i++) {
if (option[i] == "game_difficulty_new:0")difficult = 0;
if (option[i] == "game_difficulty_new:1")difficult = 0.75;
if(option[i] == "game_difficulty_new:2")difficult = 1.5;
if(option[i] == "game_difficulty_new:3")difficult = 2.25;
if(option[i] == "gfx_renderdistance_new:128")distance = 4;
if(option[i] == "gfx_renderdistance_new:160")distance = 5;
if(option[i] == "gfx_renderdistance_new:192")distance = 6;
if(option[i] == "gfx_renderdistance_new:224")distance = 7;
if(option[i] == "gfx_renderdistance_new:288")distance = 9;
if(option[i] == "gfx_renderdistance_new:352")distance = 11;
}
}
}

