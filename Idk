function showDebug() {
    ctx.runOnUiThread(new java.lang.Runnable() {
        run: function () {
            if (show == false) {
                var metrics = new android.util.DisplayMetrics();
                ctx.getWindowManager().getDefaultDisplay().getMetrics(metrics);
                var displayHeight = metrics.heightPixels;
                var displayWidth = metrics.widthPixels;

                var den = ctx.getResources().getDisplayMetrics().density;
                simpleGUI = new android.widget.PopupWindow(ctx);
                var layout = new android.widget.LinearLayout(ctx);
                textVersion = new android.widget.TextView(ctx);
                textVersion.setText("");
                textVersion.setTextSize(textSize);
                textVersion.setTypeface(mcfont);
                if(android.os.Build.VERSION.SDK_INT > 19){ // KITKAT
					textVersion.setShadowLayer(1, Math.round(textVersion.getLineHeight() / 8), Math.round(textVersion.getLineHeight() / 8), android.graphics.Color.parseColor("#FF333333"));
				}else{
					textVersion.setShadowLayer(0.0001, Math.round(textVersion.getLineHeight() / 8), Math.round(textVersion.getLineHeight() / 8), android.graphics.Color.parseColor("#FF333333"));
				}
                textVersion.setTextColor(android.graphics.Color.WHITE);
                textFPS = new android.widget.TextView(ctx);
                textFPS.setText("");
                textFPS.setTextSize(textSize);
                if(android.os.Build.VERSION.SDK_INT > 19){ // KITKAT
					textFPS.setShadowLayer(1, Math.round(textFPS.getLineHeight() / 8), Math.round(textFPS.getLineHeight() / 8), android.graphics.Color.parseColor("#FF333333"));
				}else{
					textFPS.setShadowLayer(0.0001, Math.round(textFPS.getLineHeight() / 8), Math.round(textFPS.getLineHeight() / 8), android.graphics.Color.parseColor("#FF333333"));
				}
                textFPS.setTextColor(android.graphics.Color.WHITE);
                textFPS.setTypeface(mcfont);
                textChunk = new android.widget.TextView(ctx);
                textChunk.setText("");
